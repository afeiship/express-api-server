    "pm2": "^4.2.3",
    "prettier": "^1.19.1"
  },
  "dependencies": {
    "@feizheng/next-js-core2": "^2.4.2",
    "body-parser": "^1.19.0",
    "express": "^4.17.1"
  }
}
