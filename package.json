{
  "name": "koa-api-server",
  "version": "1.0.0",
  "description": "Mini koa api server.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afeiship/koa-api-server.git"
  },
  "keywords": [
    "koa",
    "api",
    "server"
  ],
  "author": "afei",
  "bugs": {
    "url": "https://github.com/afeiship/koa-api-server/issues"
  },
  "homepage": "https://github.com/afeiship/koa-api-server#readme",
  "scripts": {
    "dev": "nodemon --exec babel-node src/app.js",
    "log:clean": "rm -rf */*.log",
    "app:start": "pm2 start --watch",
    "app:restart": "pm2 kill && pm2 start --watch",
    "app:stop": "pm2 stop sso-tokens-api",
    "app:kill": "pm2 kill",
    "app:list": "pm2 list"
  },
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
